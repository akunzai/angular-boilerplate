{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":preserveSemverRanges",
    ":widenPeerDependencies",
    ":semanticCommits",
    ":automergeMinor",
    ":automergeLinters",
    ":automergeTesters",
    ":automergeTypes",
    ":automergeRequireAllStatusChecks",
    "github>visualon/renovate-config:ng"
  ],
  "postUpdateOptions": [
    "yarnDedupeHighest"
  ],
  "packageRules": [
    {
      "description": "Require manual update the worker file for msw",
      "matchPackageNames": [
        "msw"
      ],
      "automerge": false
    }
  ]
}
