    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-preset-angular": "^12.2.3",
    "msw": "^0.49.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
